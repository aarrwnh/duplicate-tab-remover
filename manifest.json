{
	"description": "Remove tab duplicates.",
	"manifest_version": 2,
	"version": "0.0.1",
	"name": "duplicate-tab-remover",
	"author": "Wenereth Arkhilmor",
	"browser_specific_settings": {
		"gecko": {
			"id": "duplicate-tab-remover@wenereth.arkhilmor",
			"strict_min_version": "90.0"
		}
	},
	"icons": {
		"48": "trash.svg"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"32": "trash.svg"
		}
	},
	"permissions": [
		"activeTab",
		"notifications",
		"tabs"
	]
}
